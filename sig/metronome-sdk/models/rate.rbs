module MetronomeSDK
  module Models
    type rate =
      {
        rate_type: MetronomeSDK::Models::Rate::rate_type,
        credit_type: MetronomeSDK::Models::CreditTypeData,
        custom_rate: ::Hash[Symbol, top],
        is_prorated: bool,
        price: Float,
        pricing_group_values: ::Hash[Symbol, String],
        quantity: Float,
        tiers: ::Array[MetronomeSDK::Models::Tier],
        use_list_prices: bool
      }

    class Rate < MetronomeSDK::BaseModel
      attr_accessor rate_type: MetronomeSDK::Models::Rate::rate_type

      attr_reader credit_type: MetronomeSDK::Models::CreditTypeData?

      def credit_type=: (
        MetronomeSDK::Models::CreditTypeData
      ) -> MetronomeSDK::Models::CreditTypeData

      attr_reader custom_rate: ::Hash[Symbol, top]?

      def custom_rate=: (::Hash[Symbol, top]) -> ::Hash[Symbol, top]

      attr_reader is_prorated: bool?

      def is_prorated=: (bool) -> bool

      attr_reader price: Float?

      def price=: (Float) -> Float

      attr_reader pricing_group_values: ::Hash[Symbol, String]?

      def pricing_group_values=: (
        ::Hash[Symbol, String]
      ) -> ::Hash[Symbol, String]

      attr_reader quantity: Float?

      def quantity=: (Float) -> Float

      attr_reader tiers: ::Array[MetronomeSDK::Models::Tier]?

      def tiers=: (
        ::Array[MetronomeSDK::Models::Tier]
      ) -> ::Array[MetronomeSDK::Models::Tier]

      attr_reader use_list_prices: bool?

      def use_list_prices=: (bool) -> bool

      def initialize:
        (
          rate_type: MetronomeSDK::Models::Rate::rate_type,
          credit_type: MetronomeSDK::Models::CreditTypeData,
          custom_rate: ::Hash[Symbol, top],
          is_prorated: bool,
          price: Float,
          pricing_group_values: ::Hash[Symbol, String],
          quantity: Float,
          tiers: ::Array[MetronomeSDK::Models::Tier],
          use_list_prices: bool
        ) -> void
        | (?MetronomeSDK::Models::rate | MetronomeSDK::BaseModel data) -> void

      def to_hash: -> MetronomeSDK::Models::rate

      type rate_type = :FLAT | :PERCENTAGE | :SUBSCRIPTION | :CUSTOM | :TIERED

      class RateType < MetronomeSDK::Enum
        FLAT: :FLAT
        PERCENTAGE: :PERCENTAGE
        SUBSCRIPTION: :SUBSCRIPTION
        CUSTOM: :CUSTOM
        TIERED: :TIERED

        def self.values: -> ::Array[MetronomeSDK::Models::Rate::rate_type]
      end
    end
  end
end
